<Project Sdk="Microsoft.NET.Sdk.Web">

  <Import Project="..\..\common.props" />

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>AbpLteDemo.Web</RootNamespace>
    <AssetTargetFallback>$(AssetTargetFallback);portable-net45+win8+wp8+wpa81;</AssetTargetFallback>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <GenerateBindingRedirectsOutputType>true</GenerateBindingRedirectsOutputType>
    <GenerateRuntimeConfigurationFiles>true</GenerateRuntimeConfigurationFiles>
    <PreserveCompilationContext>true</PreserveCompilationContext>
    <MvcRazorExcludeRefAssembliesFromPublish>false</MvcRazorExcludeRefAssembliesFromPublish>
    <PreserveCompilationReferences>true</PreserveCompilationReferences>
    <UserSecretsId>AbpLteDemo-4681b4fd-151f-4221-84a4-929d86723e4c</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Logs\**" />
    <Compile Remove="Pages\Plugins\**" />
    <Content Remove="Logs\**" />
    <Content Remove="Pages\Plugins\**" />
    <EmbeddedResource Remove="Logs\**" />
    <EmbeddedResource Remove="Pages\Plugins\**" />
    <None Remove="Logs\**" />
    <None Remove="Pages\Plugins\**" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Pages\**\*.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Pages\**\*.css">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Serilog.AspNetCore" Version="4.1.0" />
    <PackageReference Include="Serilog.Sinks.Async" Version="1.4.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\AbpLteDemo.Application\AbpLteDemo.Application.csproj" />
    <ProjectReference Include="..\AbpLteDemo.HttpApi\AbpLteDemo.HttpApi.csproj" />
    <ProjectReference Include="..\AbpLteDemo.EntityFrameworkCore.DbMigrations\AbpLteDemo.EntityFrameworkCore.DbMigrations.csproj" />
    <ProjectReference Include="..\Indo.Abp.AspNetCore.Mvc.UI.Theme.AdminLTE\Indo.Abp.AspNetCore.Mvc.UI.Theme.AdminLTE.csproj" />
    <PackageReference Include="Volo.Abp.Autofac" Version="4.3.2" />
    <PackageReference Include="Volo.Abp.Swashbuckle" Version="4.3.2" />
    <PackageReference Include="Volo.Abp.AspNetCore.Authentication.JwtBearer" Version="4.3.2" />
    <PackageReference Include="Volo.Abp.AspNetCore.Serilog" Version="4.3.2" />
    <PackageReference Include="Volo.Abp.Identity.Web" Version="4.3.2" />
    <PackageReference Include="Volo.Abp.Account.Web.IdentityServer" Version="4.3.2" />
    <PackageReference Include="Volo.Abp.TenantManagement.Web" Version="4.3.2" />
    <PackageReference Include="Volo.Abp.SettingManagement.Web" Version="4.3.2" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Pages\PagesProjectDetail\" />
    <Folder Include="Pages\PagesProjectAdd\" />
    <Folder Include="Pages\PagesProfile\" />
    <Folder Include="Pages\PagesInvoice\" />
    <Folder Include="wwwroot\img\" />
  </ItemGroup>

</Project>
